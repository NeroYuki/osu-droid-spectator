{
  "name": "osu-droid-spectator",
  "version": "1.0.0",
  "description": "A web-based spectator client for osu!droid collaborating with multiplayer-based Discord bot",
  "scripts": {
    "build": "node src/bundler.js",
    "format-code": "npx prettier -w --loglevel=silent \"src/ts/**\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Rian8337",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@types/jquery": "^3.5.14",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "eslint": "^8.27.0",
    "prettier": "^3.0.3",
    "tsify": "^5.0.4"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "lodash.clonedeep": "^4.5.0",
    "socket.io-client": "^4.7.2",
    "typescript": "^5.0.3"
  },
  "eslintConfig": {
    "env": {
      "es2022": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "prettier": {
    "tabWidth": 4
  }
